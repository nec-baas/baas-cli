    "lint": "eslint bin lib",
    "test-jenkins": "./node_modules/.bin/istanbul cover --config=istanbul-jenkins.yml ./node_modules/.bin/_mocha test/**/*.js -- --reporter mocha-junit-reporter"
  },
  "files": [
    "bin",
    "lib",
    "extlib"
  ],
  "dependencies": {
    "@nec-baas/jssdk": "~7.5.0",
    "commander": "^2.17.1",

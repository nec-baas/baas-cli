  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.3.0",
    "istanbul": "^0.4.5",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "mongod": "^2.0.0",
    "sinon": "^6.1.5"
